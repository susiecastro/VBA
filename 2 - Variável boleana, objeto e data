'' Variável boleana

Sub Exemplo()

    Dim A As Boolean
    Dim B As Boolean
        
    A = 5 > 10
    B = 20 > 6
    
    MsgBox A
    MsgBox B
    
End Sub

'' Variável Range e Worksheet - Objetos

Sub Exemplo2()
    
    Dim r As Range
    Dim w As Worksheet
        
    Set w = Planilha2
    w.Name = "Teste"
    
    
    Set r = Range("B2:C5")
    r.Activate
    r.Value = "EstudandoVBA"
    r.Columns.AutoFit
    

End Sub

' Variável Data

Sub Exemplo3()
    
    Dim d As Date
    Dim e As Date
    
    d = "07/03/2021"
    
    ''Padrão americano na declaração da variável a mesma precisa ser colocda entre #
    e = #3/7/2023#
    
    
    Range("b7").Value = d
    Range("b8").Value = e
    
End Sub
